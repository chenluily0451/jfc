  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "gulp clean && npm run server:dev | gulp watch",
    "build": "npm run build:aot && gulp sass",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:aot": "npm run clean:aot &&webpack --config config/webpack.prod.js  --progress --profile --bail",
    "build:dev": "npm run clean:dist && npm run webpack -- --config config/webpack.dev.js --progress --profile",
    "build:docker": "npm run build:prod && docker build -t angular2-webpack-start:latest .",
    "build:prod": "npm run clean:dist && npm run webpack -- --config config/webpack.prod.js  --progress --profile --bail",
    "clean:dll": "rimraf -- dll",
    "clean:aot": "rimraf -- compiled",
    "clean:dist": "rimraf -- static",
    "clean:install": "npm set progress=false && npm install",
    "clean": "npm cache clean && rimraf -- node_modules doc coverage dist compiled dll",
    "server:dev": "npm run webpack-dev-server -- --config config/webpack.dev.js --open --progress --profile --watch",
    "webpack": "webpack ",
    "webpack-dev-server": "node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js"
  },
  "private": true,
